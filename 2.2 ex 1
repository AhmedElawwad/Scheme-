;;Write a function that takes a list of lists and returns a list of the length of each of these lists. Use the predefined Scheme function

(define (MapLen lst)
  (map length lst))
  
  ;;print - (MapLen '((a b c) (d) (e f) (g h i j) (k l)))

;;

(define (MapLen2 lst)
  (map (lambda (l) (length l))
       lst))
;; print (MapLen2 '((a b c) (d) (e f) (g h i j) (k l)))
